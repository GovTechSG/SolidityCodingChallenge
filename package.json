{
  "name": "react-box",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "server": "cd build_webpack && python -m SimpleHTTPServer 8080",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && cp ./_redirects ./build_webpack/_redirects",
    "test": "truffle test",
    "eslint": "eslint . --ext .js",
    "eslint:fix": "eslint . --ext .js --fix",
    "solium": "solium -d contracts/",
    "solium:fix": "solium -d contracts/ --fix",
    "lint": "npm run eslint && npm run solium",
    "prettier": "prettier --write **/*.sol",
    "lint:fix": "npm run prettier && npm run eslint:fix && npm run solium:fix",
    "compile:contracts": "rm -rf build && npx truffle compile",
    "deploy:contracts": "npm run compile:contracts && node scripts/deploy_contracts.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bignumber": "^1.1.0",
    "bignumber.js": "^4.1.0",
    "bootstrap": "^3.3.7",
    "color-map": "^0.5.9",
    "colors": "^1.1.2",
    "devtools-detect": "^2.2.0",
    "dotenv": "^2.0.0",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.2",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.17",
    "moment": "^2.21.0",
    "nan": "^2.10.0",
    "prompt": "^1.0.0",
    "rainbow-color": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.3.5",
    "react-highlight": "^0.10.0",
    "react-inform": "^0.2.2",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-sidebar": "^2.3.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rlp": "^2.0.0",
    "sha3": "^1.2.2",
    "truffle": "^5.0.0",
    "truffle-deployer": "^2.0.0",
    "web3": "1.0.0-beta.37",
    "web3-core-helpers": "1.0.0-beta.37",
    "web3-core-method": "1.0.0-beta.37",
    "web3-utils": "1.0.0-beta.37",
    "zeppelin-solidity": "1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.1",
    "bip39": "2.3.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "6.4.1",
    "ethereumjs-testrpc": "^2.2.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "husky": "^1.3.1",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "prettier": "^1.15.3",
    "prettier-plugin-solidity": "^1.0.0-alpha.16",
    "promise": "7.1.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "solium": "^1.2.1",
    "style-loader": "0.13.1",
    "truffle-contract": "^3.0.0",
    "truffle-solidity-loader": "0.0.8",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
